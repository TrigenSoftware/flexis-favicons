{
  "name": "@flexis/favicons",
  "version": "1.1.2",
  "description": "Best lib to generating favicons for modern web.",
  "author": "dangreen",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TrigenSoftware/flexis-favicons.git"
  },
  "bugs": {
    "url": "https://github.com/TrigenSoftware/flexis-favicons/issues"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "generateDocs": "typedoc ./src --out ./docs --ignoreCompilerErrors --excludeExternals --mode modules; touch docs/.nojekyll",
    "lint": "tslint -p . -t stylish 'src/**/*.{ts,tsx}'",
    "jest": "NODE_ENV=test jest -c jest.config.json",
    "checkSize": "size-limit",
    "test": "npm run lint && npm run jest && npm run build && npm run checkSize",
    "build": "rollup -c",
    "watch": "rollup -c -w",
    "coverage": "cat ./coverage/lcov.info | coveralls",
    "cleanPublish": "npm run test && clean-publish"
  },
  "keywords": [
    "favicon",
    "ico",
    "generator"
  ],
  "dependencies": {
    "@babel/runtime": "^7.2.0",
    "@types/node": "^10.12.18",
    "@types/sharp": "^0.21.0",
    "@types/vinyl": "^2.0.2",
    "sharp": "^0.21.1",
    "svg2img": "^0.5.0",
    "to-ico": "^1.1.5",
    "vinyl": "^2.1.0"
  },
  "devDependencies": {
    "@babel/core": "7.3.3",
    "@types/jest": "^23.3.11",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-preset-trigen": "^1.8.0",
    "browserslist-config-trigen": "^1.1.0",
    "clean-publish": "^1.1.0",
    "coveralls": "^3.0.0",
    "eslint": "^5.12.0",
    "eslint-config-trigen": "^3.2.1",
    "husky": "^1.3.1",
    "jest": "^23.5.0",
    "lint-staged": "^8.1.0",
    "rollup": "^1.0.2",
    "rollup-plugin-babel": "^4.2.0",
    "rollup-plugin-commonjs": "^9.1.6",
    "rollup-plugin-eslint": "^5.0.0",
    "rollup-plugin-tslint": "^0.1.35",
    "rollup-plugin-typescript2": "^0.18.1",
    "size-limit": "^0.21.1",
    "ts-jest": "^23.10.5",
    "tslint": "^5.12.0",
    "tslint-config-trigen": "^3.0.7",
    "typedoc": "^0.14.0",
    "typescript": "^3.2.2"
  },
  "files": [
    "lib"
  ]
}
